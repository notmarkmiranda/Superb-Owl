<h2>Sign In</h2>
<%= form_with url: sign_in_path do |f| %>
  <div>
    <%= f.label :email, "Email Address" %>
    <%= f.text_field :email %>
  </div>
  <div>
    <%= f.label :password, "Password" %>
    <%= f.password_field :password %>
  </div>
  <div>
    <%= f.submit "Sign In" %>
  </div>
<% end %>
